Public class Solution {

	public static String removeConsecutiveDuplicates(String s) {
		String str="";
		for(int i=0;i<s.length();i++){
        
        	if(i<s.length()-1&&s.charAt(i)==s.charAt(i+1)){
					 continue;
				  }
				  else{
				  str=str+s.charAt(i);
				  }
		}
    return str;
	}

}
