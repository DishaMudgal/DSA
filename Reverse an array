class Main {
    public static void main(String[] args) {
       int arr[]={2,4,5,6,7,8,9};
       int temp=0;
       int n=arr.length;
       for(int i=0;i<=n/2-1;i++){
           temp=arr[i];
           arr[i]=arr[n-i-1];
           arr[n-i-1]=temp;
       }
       for(int k:arr){
           System.out.print(k);
       }
}
}
